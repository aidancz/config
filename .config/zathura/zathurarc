# man zathura zathurarc

set recolor true
set recolor-darkcolor  "#9e9e9e"
set recolor-lightcolor "#2b2b2b"
# set recolor-darkcolor  "#616161"
# set recolor-lightcolor "#d4d4d4"

map <F1> quit
map <C-Esc> quit

map g goto top
# map e scroll down
# map y scroll up
map j scroll half-down
map k scroll half-up
map f scroll full-down
map d scroll full-up
map w navigate next
map b navigate previous

map <C-a> toggle_page_mode
